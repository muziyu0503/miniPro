/**app.wxss**/
.flex{
  display: flex;
  justify-content: space-between;
  align-content: center;
  font-size: 30rpx;
  padding: 20rpx 16rpx;
  height: 100rpx;
  box-sizing: border-box;
}
.waiting_img {
  text-align: center;
  width:100%;
  height:120rpx;
}
.waiting_img image{
  width: 62rpx;
  height: 63rpx;
  text-align: center;
}
.endTips {
  width: 100%;
  color: #999;
  text-align: center;
  font-size: 26rpx;
  line-height: 80rpx;
}
/* 公共布局 */
.dis-flex {
  display: flex;
  display: -webkit-flex;
}
.dis-flex-center {
  justify-content: center;
  align-items: center;
}
.opacityBtn {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  opacity: 0;
  margin: 0;
  padding: 0;
  display: block;
  width: 100%;
  height: 100%;
  z-index: 2;
  background: tomato;
}